<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://127.0.0.1:12004/ServerOperatorService"
                binding="netTcpBinding"
                bindingConfiguration="duplexnetTcpBinging"
                contract="WCF.Interfaces.IServerOperator"
                name="defaultEndpoint"/>
    </client>
    <bindings>
      <netTcpBinding>
        <binding name="duplexnetTcpBinging">
          <security mode="None"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>