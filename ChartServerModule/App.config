<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <bindings>
      <netTcpBinding >
        <binding name="duplexnetTcpBinding">
          <security mode="None"></security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="duplexWcf" >
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="ChartServerModule.ServerOperatorService"
               behaviorConfiguration="duplexWcf">
        <endpoint
          address="net.tcp://127.0.0.1:12004/ServerOperatorService"
          binding="netTcpBinding"
          bindingConfiguration="duplexnetTcpBinding"
          contract="WCF.Interfaces.IServerOperator"/>
      </service>
    </services>
  </system.serviceModel>
</configuration>